<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Selenium.Firefox.WebDriver</id>
    <version>0.30.0</version>
    <authors>jbaranda</authors>
    <summary>Selenium Firefox WebDriver Marionette (Win64)</summary>
    <description>
	Repurposed nuget package of Selenium.WebDriver.ChromeDriver but for Firefox Driver instead
	Install Firefox Driver(Win64) Marionette for Selenium WebDriver into your Unit Test Project.
	"geckodriver.exe" is copied to bin folder from package folder when the build process.
	NuGet package restoring ready, and no need to commit "geckodriver.exe" binary into source code control repository.
    </description>
    <language>en-US</language>
    <projectUrl>https://github.com/jbaranda/nupkg-selenium-webdrivers</projectUrl>
    <license type="expression">Unlicense</license>
    <tags>Selenium WebDriver Firefox</tags>
    <releaseNotes>v0.30.0 - Firefox Driver 0.30.0 release - https://github.com/mozilla/geckodriver/releases/tag/v0.30.0
    </releaseNotes>
  </metadata>
  <files>
    <file src="geckodriver.exe" target="driver"/>
    <file src="Selenium.Firefox.WebDriver.targets" target="build"/>
  </files>
</package>
